        <servlet-name>User_order</servlet-name>
        <url-pattern>/User_order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>remove_order</servlet-name>
        <url-pattern>/remove_order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_login</servlet-name>
        <url-pattern>/admin_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_addproduct</servlet-name>
        <url-pattern>/admin_addproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_review</servlet-name>
        <url-pattern>/add_review</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
