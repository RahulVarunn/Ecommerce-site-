<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>
            main_page.jsp
        </welcome-file>
    </welcome-file-list>
    <context-param>
        <param-name>sitename</param-name>
        <param-value>JAPAN</param-value>
    </context-param>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>signin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_profile</servlet-name>
        <servlet-class>update_profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cart_add</servlet-name>
        <servlet-class>cart_add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>change</servlet-name>
        <servlet-class>change</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_cart</servlet-name>
        <servlet-class>user_cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>order_detail</servlet-name>
        <servlet-class>order_detail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>check_session</servlet-name>
        <servlet-class>check_session</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>remove_prod</servlet-name>
        <servlet-class>remove_prod</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>product_detail</servlet-name>
        <servlet-class>product_detail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signout</servlet-name>
        <servlet-class>signout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User_order</servlet-name>
        <servlet-class>User_order</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>remove_order</servlet-name>
        <servlet-class>remove_order</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_login</servlet-name>
        <servlet-class>admin.admin_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_addproduct</servlet-name>
        <servlet-class>admin.admin_addproduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_review</servlet-name>
        <servlet-class>add_review</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_profile</servlet-name>
        <url-pattern>/update_profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cart_add</servlet-name>
        <url-pattern>/cart_add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>change</servlet-name>
        <url-pattern>/change</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_cart</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>order_detail</servlet-name>
        <url-pattern>/order_detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>check_session</servlet-name>
        <url-pattern>/check_session</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>remove_prod</servlet-name>
        <url-pattern>/remove_prod</url-pattern>
    </servlet-mapping>
